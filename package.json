{
	"name": "bare-routes",
	"version": "0.3.1",
	"description": "A bare-bones React client-side page router that delegates the actual routing and page loading to user code and only handles history manipulation",
	"keywords": [
		"router",
		"react"
	],
	"repository": "github:cyco130/bare-routes",
	"homepage": "https://github.com/cyco130/bare-routes#readme",
	"files": [
		"dist/**/*.js",
		"dist/esm/*.d.ts",
		"README.md",
		"LICENSE"
	],
	"main": "dist/lib/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"scripts": {
		"dev": "vite --config vite.config.ts",
		"prepublishOnly": "pnpm run build",
		"build": "rimraf dist && cross-env BROWSERSLIST_ENV=production rollup --config rollup.config.js",
		"test": "jest --coverage",
		"format": "prettier --write .",
		"typecheck": "tsc -p . --noEmit",
		"lint": "eslint --max-warnings=0",
		"prepare": "husky install"
	},
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"peerDependencies": {
		"react": "^16.8 || ^17"
	},
	"devDependencies": {
		"@babel/core": "^7.14.0",
		"@babel/preset-env": "^7.14.1",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/register": "^7.13.16",
		"@babel/runtime": "^7.14.0",
		"@testing-library/jest-dom": "^5.12.0",
		"@testing-library/react": "^11.2.6",
		"@types/jest": "^26.0.23",
		"@types/react-dom": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"@wessberg/rollup-plugin-ts": "^1.3.14",
		"cross-env": "^7.0.3",
		"eslint": "^7.26.0",
		"eslint-plugin-only-warn": "^1.0.2",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"lint-staged": "^11.0.0",
		"prettier": "^2.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.47.0",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.2.4",
		"vite": "^2.2.4"
	},
	"dependencies": {
		"@types/react": "^17.0.5"
	}
}
